<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
  <item app:tsquare_state_range_middle="true">
    <color android:color="@color/middle_color"/>
  </item>
  <item android:state_selected="true">
    <color android:color="@color/colorPrimary"/>
  </item>
  <item android:state_pressed="true">
    <color android:color="@color/colorPrimary"/>
  </item>
  <item app:tsquare_state_current_month="false">
    <color android:color="@color/calendar_inactive_month_bg"/>
  </item>
    <item app:tsquare_state_selectable="false">
    <color android:color="@color/calendar_inactive_month_bg"/>
  </item>
  <item app:tsquare_state_highlighted="true">
    <color android:color="@color/calendar_highlighted_day_bg"/>
  </item>
  <item app:tsquare_state_today="true">
    <color android:color="@color/calendar_active_month_bg"/>
  </item>
  <item>
    <color android:color="@color/calendar_active_month_bg"/>
  </item>
</selector>
